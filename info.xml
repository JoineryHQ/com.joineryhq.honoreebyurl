<?xml version="1.0"?>
<extension key="com.joineryhq.honoreebyurl" type="module">
  <file>honoreebyurl</file>
  <name>Honoree by URL</name>
  <description>Allow creation of soft credits by inserting appropriate query parameters into the contribution page URL.</description>
  <license>GPL-3.0</license>
  <maintainer>
    <author>Joinery</author>
    <email>info@JoineryHQ.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/JoineryHQ/com.joineryhq.honoreebyurl</url>
    <url desc="Documentation">https://github.com/JoineryHQ/com.joineryhq.honoreebyurl</url>
    <url desc="Support">https://github.com/JoineryHQ/com.joineryhq.honoreebyurl/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/gpl-3.0.html</url>
  </urls>
  <releaseDate>2021-06-01</releaseDate>
  <version>1.0</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>Developed by Joinery for The Cadets.</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Honoreebyurl</namespace>
  </civix>
</extension>
